{
  "name": "flatmate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "reset": "npm run clean:all && npm run build:all && npm run start:all",
    "clean:all": "rm -rf dist tsconfig.node.tsbuildinfo tsconfig.tsbuildinfo",
    "build:all": "npm run build:css && npm run build:storybook && npm run build:app",
    "build:app": "tsc --build tsconfig.json && tsc --build tsconfig.node.json && vite build",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/output.css --minify",
    "build:storybook": "storybook build",
    "start:all": "concurrently \"npm run start:express\" \"npm run start:storybook\"",
    "start:express": "ts-node-esm ./src/server.ts",
    "start:storybook": "storybook dev -p 6006",
    "dev": "npm run build-all && wait-on dist && npm run start-all"
  },
  "dependencies": {
    "@nlux/openai-react": "^2.5.7",
    "@nlux/react": "^2.5.7",
    "@nlux/themes": "^2.5.10",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "init": "^0.1.2",
    "lucide-react": "^0.394.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-actions": "^8.1.9",
    "@storybook/addon-controls": "^8.1.9",
    "@storybook/addon-designs": "^8.0.2",
    "@storybook/addon-docs": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-mdx-gfm": "^8.1.9",
    "@storybook/addon-onboarding": "^8.1.9",
    "@storybook/addon-outline": "^8.1.9",
    "@storybook/addon-storysource": "^8.1.9",
    "@storybook/addon-themes": "^8.1.9",
    "@storybook/addon-viewport": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/cli": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "storybook": "^8.1.9",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "ts-node-esm": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  }
}